{
    "name": "antidot-fw/doctrine",
    "description": "Antidot Framework doctrine orm integration library",
    "keywords": [
        "psr-11"
    ],
    "type": "library",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "kpicaza"
        }
    ],
    "require": {
        "php": "^7.2.13",
        "psr/container": "^1.0.0",
        "dasprid/container-interop-doctrine": "^1.1.0"
    },
    "require-dev": {
        "phpro/grumphp": "^0.15.0",
        "phpstan/phpstan": "^0.11.5",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Antidot\\Persistence\\Doctrine\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AntidotTest\\Persistence\\Doctrine\\": "test"
        }
    },
    "scripts": {
        "check-all": [
            "@cs-check",
            "@test",
            "@inspect"
        ],
        "cs-check": "phpcs src --colors",
        "cs-fix": "phpcbf src --colors",
        "inspect": "phpstan analyse src -l7 --ansi",
        "test": "phpunit --colors=always"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "zf": {
            "config-provider": "Antidot\\Persistence\\Doctrine\\Container\\Config\\ConfigProvider"
        }
    }
}
